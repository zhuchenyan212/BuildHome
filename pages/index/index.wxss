/**index.wxss**/
@import "../wxParse/wxParse.wxss";

page {
  background: #f5f5f7;
  width: 100%;
  overflow-x: hidden;
  z-index: 999;
}

.container {
  z-index: 999;
}

.container .swiper-slider image {
  width: 100%;
  height: 100%;
  display: inline-block;
  overflow: hidden;
}

.container .ui-grids {
  padding: 10rpx 0rpx 10rpx;
  background: #fff;
}

.container .ui-grids .grid {
  width: 130rpx;
  margin-right: 20rpx;
  float: left;
  text-align: center;
  margin-left: 15px;
}

.container .ui-grids .last-grid {
  margin-right: 0;
}

.container .ui-grids .ui_grid-img {
  height: 90rpx;
  margin: 0 auto;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.container .ui-grids .ui_grid-img image {
  width: 76rpx;
  height: 76rpx;
}

.container .ui-grids .ui_grid-label {
  text-align: center;
  font-size: 24rpx;
  margin-top: 10rpx;
}

.container .floor {
  height: 260rpx;
  padding: 0 24rpx;
  background: #fff;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}

.container .floor_box .title {
  height: 80rpx;
  line-height: 80rpx;
}

.container .floor_box .title .right {
  font-size: 20rpx;
  font-weight: bold;
}

.container .floor .list {
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
}

.container .floor .list .scrollBox {
  width: 100%;
  height: 150rpx;
  white-space: nowrap;
}

.container .floor .list .scrollBox .item {
  width: 330rpx;
  height: 150rpx;
  box-sizing: border-box;
  overflow: hidden;
  display: inline-block;
  margin-left: 24rpx;
}

.container .floor .list .scrollBox .item image {
  width: 100%;
  height: 100%;
  display: block;
}

.remai {
  color: #342b2b;
  font-size: 13px;
}

.container .catagory {
  height: 480rpx;
  padding: 0 24rpx;
  background: #fff;
  margin-bottom: 10rpx;
  margin-top: 10rpx;
}

.container .catagory .title {
  text-align: center;
  height: 80rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #342b2b;
}

.container .catagory .floor_box {
  padding: 0 26rpx;
}

.container .catagory .p_box {
  padding: 2px 20px;
}

.line {
  color: #707070;
}

.red {
  color: red;
}

.container .catagory .p_box .formitem {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  margin: 10px auto;
}

.container .catagory .p_box .formitem .tit {
  width: 20%;
  text-align: center;
}

.container .catagory .p_box .formitem .oinput {
  width: 80%;
}

.container .catagory .p_box .formitem input {
  width: 100%;
  height: 70rpx;
  background: #f2f2f2;
  border-radius: 5px;
  border: 1px solid rgba(222, 219, 217, 1);
  padding-left: 20rpx;
}

.container .catagory .p_box button {
  width: 100%;
  height: 35px;
  line-height: 35px;
  color: #fff;
  background: rgba(220, 127, 32, 1);
  opacity: 1;
  border-radius: 5px;
  font-size: 16px;
  margin-top: 20px;
}

.notice_wrap {
  height: 60rpx;
  margin-top: 15px;
  text-align: center;
}

.container .swiper-vertical {
  width: 100%;
  float: left;
}

.container .swiper-vertical .slide-title {
  width: 20%;
  height: 60rpx;
  padding-left: 20rpx;
  float: left;
  margin-top: 9rpx;
}

.container .swiper-vertical .slide-title image {
  width: 32rpx;
  height: 32rpx;
}

.container .swiper-vertical .swiper-info {
  width: 68%;
  height: 60rpx;
  float: left;
}

.container .swiper-vertical .swiper-info swiper {
  height: 44rpx;
  line-height: 44rpx;
  font-size: 24rpx;
  color: #342b2b;
}

.container .fuwu image {
  width: 100%;
  height: 220rpx;
}

.container .floors_box {
  width: 100%;
  height: 400rpx;
}

.container .floors_box .title {
  height: 66rpx;
  line-height: 66rpx;
  padding: 0 24rpx;
  background: #fff;
  /* margin-top: 10rpx; */
}

.container .floors_box .title .right {
  font-size: 26rpx;
  font-weight: bold;
  color: #000;
}

.container .floors_box .item {
  width: 100%;
  height: 320rpx;
  background: #fff;
}

.container .floors .floors_box .item image {
  width: 100%;
  height: 100%;
  display: block;
}

/* coupon */

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  background-color: rgba(0, 0, 0, 0.65);
  z-index: 998;
  left: 0;
  top: 0;
}

.couponWrap {
  position: absolute;
  top: 38%;
  left: 21.5%;
  z-index: 999;
  width: 100%;
}

.item-ans {
  display: flex;
  flex-grow: row no-wrap;
  justify-content: space-between;
  padding: 30rpx;
  box-sizing: border-box;
  height: 180rpx;
  align-items: center;
  margin-top: 20rpx;
  position: relative;
}

.expertInfo {
  font-size: 12px;
  flex-grow: 2;
  color: #fff;
  line-height: 1.5em;
  padding-left: 30rpx;
}

.expertInfo .name {
  font-size: 40rpx;
  margin-bottom: 6px;
}

.expertInfo .name text {
  font-size: 28rpx;
}

.tab-content {
  margin-top: 80rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.scoll-h {
  height: 100%;
}

.closeCoupon {
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  top: 37%;
  right: 14%;
  z-index: 999;
}

.bgon {
  height: 200rpx;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
}

/* newadd */

.quanwrap {
  width: 100%;
  height: 128rpx;
  margin-bottom: 12rpx;
  padding: 16rpx 20rpx;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.quanwrap .quanItem {
  height: 96rpx;
  border-radius: 10rpx;
  background: #ccc;
  /* display: flex; */
  /* align-items: stretch; */
  /* flex-direction: column; */
  width: 32%;
  font-size: 18rpx;
}

.quanwrap .quanItem view {
  color: #fff;
}

.leftquan {
  width: 90%;
  height: 96rpx;
  float: left;
  border-radius: 10rpx 0rpx 0rpx 10rpx;
  background: #dc7f20;
}

.rightquan {
  width: 10%;
  height: 96rpx;
  float: left;
  border-radius: 0rpx 10rpx 10rpx 0rpx;
  background: #342b2b;
  text-align: center;
}

.money {
  width: 40%;
  height: 96rpx;
  float: left;
  padding-left: 10rpx;
}

.use {
  width: 60%;
  height: 96rpx;
  float: left;
  padding-left: 5rpx;
  padding-top: 8rpx;
}

.dataquan {
  font-size: 6px;
  height: 20rpx;
  background: #fff;
  color: #dc7f20 !important;
  line-height: 20rpx;
  padding: 0 5rpx;
}

.remen {
  width: 100%;
  height: 684rpx;
  background: #fff;
  padding: 0 16rpx 0 16rpx;
  margin-bottom: 10rpx;
}

.remen .remenTitle {
  height: 50rpx;
  line-height: 65rpx;
  padding: 0 12rpx;
  font-size: 26rpx;
  background: #fff;
}

.remen .productItem {
  width: 226rpx;
  float: left;
  margin-left: 12rpx;
  margin-top: 14rpx;
  background: #f7f6f9;
}

.remen .productItem image {
  width: 100%;
  height: 226rpx;
}

.remen .productItem .proinfo {
  font-size: 16rpx;
  padding: 0 8rpx 4rpx 8rpx;
}

.remen .productItem .proinfo .protitle {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
}

.remen .productItem .proinfo .promoney {
  color: #dc7f20;
}

.remen .more {
  color: #dc7f20;
  font-size: 20rpx;
  width: 100%;
  text-align: center;
  height: 40rpx;
  line-height: 40rpx;
  float: left;
}

.opaci {
  opacity: 0.5;
}

/* 弹窗 */

.showToasts {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  text-align: center;
  background-color: rgba(0, 0, 0, .65)
}

.showToasts ::after {
  content: "";
  display: inline-block;
  height: 100%;
  width: 0;
  vertical-align: middle;
}

.covers {
  width: 524rpx;
  height: 482rpx;
  background-color: #DC7F20 !important;
  display: inline-block;
  vertical-align: middle;
  position: relative;
  margin-top: 43%;
  border-radius: 10rpx;
}

.worth {
  font-size: 78rpx;
  color: #fff;
  padding: 30rpx 0 20rpx;
}

.content {
  width: 85%;
  max-height: 160rpx;
  font-size: 28rpx;
  color: #fff;
  margin: auto;
  text-align: center;
  border: 1px dashed #fff;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
}

.attrcoupon {
  width: 100%;
  height: 128rpx;
  line-height: 128rpx;
  background: #342B2B;
  color: #fff;
  text-align: center;
  font-size: 50rpx;
  position: absolute;
  bottom: 0;
}