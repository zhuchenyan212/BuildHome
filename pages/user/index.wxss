/* pages/user/index.wxss */

page {
  background: #f5f5f7;
  width: 100%;
  overflow-x: hidden;
}

.container {
  margin-bottom: 120rpx;
  background: #fff;
}

.container .headnav {
  height: 555rpx;
  background: #fff;
}

.container .headnav .headBg {
  width: 100%;
  height: 400rpx;
  background: #342b2b;
  text-align: center;
  position: relative;
}

.headImg {
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  border: 4rpx solid rgba(220, 127, 32, 1);
  background-size: cover;
  background-image: url("data:image/png;base64,");
}

.container .headnav .headBg .dingwei image {
  position: absolute;
  right: 25rpx;
  top: 25rpx;
  width: 45rpx;
  height: 45rpx;
}

.container .headnav .headBg .dingwei .infoNum {
  position: absolute;
  right: 5rpx;
  top: 10rpx;
  width: 35rpx;
  height: 35rpx;
  color: #fff;
  background: red;
  border-radius: 50%;
  text-align: center;
  font-size: 26rpx;
  line-height: 30rpx;
}

.container .headnav .headBg .touxing {
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  border: 4rpx solid rgba(220, 127, 32, 1);
}

.container .headnav .headBg .fots {
  color: #dc7f20;
  font-size: 26rpx;
}

.container .headnav .headBg .fot {
  color: #dc7f20;
  font-size: 26rpx;
  width: 200rpx;
}

.container .headnav .headBg .item {
  width: 50%;
  margin: 10rpx 34%;
  text-align: center;
}

.container .headnav .headBg input {
  float: left;
}

.container .headnav .headBg .changephone {
  width: 30rpx;
  height: 30rpx;
  float: left;
  margin-top: 8rpx;
}

.container .headnav .carditem {
  width: 90%;
  height: 228rpx;
  background: #fff;
  position: absolute;
  top: 288rpx;
  left: 40rpx;
  border-radius: 10rpx;
  box-shadow: 0px 3px 6px rgba(0, 0, 0, 0.16);
}

.container .headnav .carditem .font {
  margin: 24rpx 32rpx;
}

.container .headnav .carditem .ui-grids {
  padding: 10rpx 0rpx 10rpx;
  background: #fff;
}

.container .headnav .carditem .ui-grids .grid {
  width: 20%;
  margin-right: 15rpx;
  float: left;
  text-align: center;
  margin-left: 15rpx;
}

.container .headnav .carditem .ui-grids .last-grid {
  margin-right: 0;
}

.container .headnav .carditem .ui-grids .ui_grid-img {
  height: 60rpx;
  margin: 0 auto;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.container .headnav .carditem .ui-grids .ui_grid-img image {
  width: 50rpx;
  height: 50rpx;
}

.container .headnav .carditem .ui-grids .ui_grid-label {
  text-align: center;
  font-size: 24rpx;
  margin-top: 10rpx;
  color: #342b2b;
}

.container .listmenu {
  width: 100%;
  height: auto;
}

.container .listmenu .menu-item {
  display: block;
  width: 100%;
  height: 93rpx;
  line-height: 93rpx;
  padding-left: 40rpx;
  background: #fff;
  margin-bottom: 10rpx;
  box-sizing: border-box;
}

.container .listmenu .menu-item image {
  width: 40rpx;
  height: 40rpx;
  margin-top: 28rpx;
  float: left;
}

.container .listmenu .menu-item text {
  font-size: 26rpx;
  color: #342b2b;
  padding-left: 20rpx;
}

.getInfo {
  width: 185rpx;
  height: 46rpx;
  line-height: 46rpx;
  background: rgba(255, 255, 255, 1);
  opacity: 1;
  border-radius: 5px;
  font-size: 28rpx;
  padding: 0;
  border: none;
  margin-top: 15rpx;
}

.btnSub {
  text-align: left;
  line-height: 1;
  font-size: 13px;
  padding-left: 0px;
  padding-right: 0px;
  font-weight: normal;
  overflow: visible;
  display: inline;
  background: #fff;
}

.btnSub::after {
  border: none;
}

.bottomV {
  width: 100%;
  text-align: center;
  margin-bottom: 60rpx;
}

.bottomInfo {
  font-size: 24rpx;
  color: #322926;
}
